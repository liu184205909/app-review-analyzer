    "@types/nodemailer": "^7.0.3",
    "@types/stripe": "^8.0.416",
    "autoprefixer": "^10.4.20",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "d3-cloud": "^1.2.7",
    "franc-min": "^6.2.0",
    "google-play-scraper": "^9.1.3",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.460.0",
    "next": "14.2.18",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.10",
