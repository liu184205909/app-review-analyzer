{
  "name": "app-review-analyzer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && node -e \"global.self = global;\" && next build",
    "start": "node -e \"global.self = global;\" && next start",
    "lint": "next lint",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "analyze:perf": "npm run build && npm run analyze:bundle",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "vercel-build": "prisma generate && node vercel-setup.js && SKIP_ENV_VALIDATION=true DATABASE_URL='postgresql://placeholder:placeholder@localhost:5432/placeholder' next build"
  },
  "dependencies": {
    "@paypal/paypal-server-sdk": "^2.0.0",
